   "source": [
    "words = [\"a\", \"b\", \"a\", \"c\", \"d\", \"a\"] # sample input\n",
    "\n",
    "def count(words):\n",
    "\tcounts = {}\n",
    "\n",
    "\tfor w in words:\n",
    "\t\tif w not in counts.keys():\n",
    "\t\t\tcounts[w] = 1\n",
    "\t\telse:\n",
    "\t\t\tcounts[w] += 1\n",
    "\n",
    "\treturn counts\n"
   ]
